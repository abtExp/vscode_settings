{
    "workbench.iconTheme": "vscode-great-icons",
    "editor.tabCompletion": true,
    "editor.fontLigatures": true,
    "terminal.integrated.fontLigatures": true,
    "editor.fontFamily": "Fira Code",
    "workbench.startupEditor": "newUntitledFile",
    "editor.minimap.enabled": false,
    "window.zoomLevel": 0,
    "terminal.integrated.cursorBlinking": true,
    "editor.tokenColorCustomizations": {
        "comments": "#2bcaa2",
        "functions": "#2685ff",
        "keywords":"#FF4040",
        "strings": "#338A84",
        "numbers": "#aaa",
        "types": "#2AE28C",
        "variables": "#FDEC89"
    },
    "workbench.colorCustomizations": {
        "activityBar.background": "#2bcaa2",
        "activityBar.foreground": "#000",
        "activityBarBadge.background": "#FF775F",
        "editor.background": "#333",
        "editor.lineHighlightBackground": "#335",
        "editor.hoverHighlightBackground": "#333",
        "editor.selectionBackground": "#263",
        "editor.inactiveSelectionBackground": "#444",
        "activityBar.border": "#111",
        "editorBracketMatch.background": "#28a",
        "editorBracketMatch.border": "#359",
        "editorCursor.foreground": "#FFD30D",
        "editorGroup.background": "#FF775F",
        "editorLineNumber.foreground": "#ddd",
        "editorSuggestWidget.border": "#fff",
        "scrollbar.shadow": "#000",
        "scrollbarSlider.activeBackground": "#2bcaa2",
        "scrollbarSlider.background": "#222",
        "scrollbarSlider.hoverBackground": "#FFD30D",
        "sideBar.background": "#333",
        "sideBar.foreground": "#fff",
        "sideBarSectionHeader.background": "#FF775F",
        "sideBar.border": "#222",
        "statusBar.background": "#111",
        "statusBar.noFolderBackground": "#2bcaa2",
        "tab.activeBackground": "#FF775F",
        "tab.inactiveBackground": "#333",
        "tab.activeBorder": "#FF775F",
        "tab.border": "#333",
        "tab.unfocusedActiveForeground": "#ddd",
        "terminal.background": "#333",
        "terminal.foreground":"#2bcaa2"
    }
}
